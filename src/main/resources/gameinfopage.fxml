<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefWidth="1000.0" style="-fx-background-color: #FAF1E6;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.kuleuven.VGHF.controller.GameInfoController">
   <children>
      <HBox prefHeight="200.0" prefWidth="500.0">
         <children>
            <AnchorPane maxWidth="5.0" minWidth="5.0" prefWidth="5.0" />
            <Button fx:id="btnExit" minWidth="50.0" mnemonicParsing="false" stylesheets="@styles.css" text="Exit" />
            <AnchorPane maxWidth="5.0" minWidth="5.0" prefWidth="5.0" />
            <VBox prefHeight="200.0" prefWidth="2000.0">
               <children>
                  <StackPane maxHeight="25.0" prefHeight="25.0" prefWidth="2000.0" style="-fx-background-color: #064420;">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="All copies registered for this game" />
                     </children>
                  </StackPane>
                  <TableView fx:id="tblCopiesTable" minHeight="50.0" prefHeight="200.0" prefWidth="500.0">
                     <columns>
                        <TableColumn prefWidth="75.0" text="C1" />
                        <TableColumn prefWidth="75.0" text="C2" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <AnchorPane minWidth="5.0" prefHeight="200.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
