<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="parentPane" maxHeight="2000.0" maxWidth="2000.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FAF1E6;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter email address here please" textAlignment="CENTER" wrappingWidth="312.03125">
            <font>
                <Font size="24.0" />
            </font>
        </Text>
      <AnchorPane prefHeight="150.0" prefWidth="200.0" />
      <TextField fx:id="txtEmail" maxWidth="300.0" minWidth="50.0" prefWidth="200.0" promptText="Enter email address" />
      <AnchorPane maxHeight="20.0" prefHeight="20.0">
         <children>
            <PasswordField fx:id="txtPassword" layoutX="-1.0" layoutY="-2.0" prefHeight="25.0" prefWidth="300.0" promptText="put your weak ass password here" />
         </children></AnchorPane>
      <Button fx:id="btnLogin" mnemonicParsing="false" prefWidth="100.0" stylesheets="@styles.css" text="Login" />
   </children>
</VBox>
