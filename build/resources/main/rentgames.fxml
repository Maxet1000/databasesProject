<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FAF1E6;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.kuleuven.VGHF.controller.RentgamesController">
   <children>
      <HBox prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
             <Text fx:id="txtRentgames" strokeType="OUTSIDE" strokeWidth="0.0" text="Filters hier?" wrappingWidth="150" />
             <VBox HBox.hgrow="ALWAYS" prefHeight="600">
                 <children>
                     <TableView fx:id="tblRent" prefHeight="600" prefWidth="200">
                         <columns>
                             <TableColumn prefWidth="75.0" text="C1" />
                             <TableColumn prefWidth="75.0" text="C2" />
                         </columns>
                     </TableView>
                 </children>
             </VBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Winkelkar hier??" wrappingWidth="150" />
         </children>
      </HBox>
   </children>
</VBox>
