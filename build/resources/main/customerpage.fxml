<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox style="-fx-background-color: #FAF1E6;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
   <children>
      <Text fx:id="txt" strokeType="OUTSIDE" strokeWidth="0.0" text="Hier kan een customer dan al zijn aankopen bekijken en shiii" />
      <HBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <StackPane maxHeight="25.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #064420;">
                     <children>
                        <Text fill="WHITE" layoutX="30.0" layoutY="17.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Balance" />
                     </children>
                  </StackPane>
                  <AnchorPane maxHeight="50.0" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: #064420; -fx-border-width: 2;">
                     <children>
                        <Text fx:id="txtBalance" layoutX="44.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="69" />
                     </children>
                  </AnchorPane>
                  <Button fx:id="btnAddBalance" maxWidth="200.0" minWidth="100.0" mnemonicParsing="false" prefWidth="100.0" stylesheets="@styles.css" text="Add to Balance" />
               </children>
            </VBox>
            <AnchorPane maxWidth="5.0" minWidth="5.0" prefHeight="200.0" prefWidth="5.0" />
            <VBox prefHeight="200.0">
               <children>
                  <StackPane maxHeight="25.0" prefHeight="25.0" prefWidth="2000.0" style="-fx-background-color: #064420;">
                     <children>
                        <Text fill="WHITE" layoutY="17.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Copies currently in rent" />
                     </children>
                  </StackPane>
                  <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <AnchorPane maxWidth="5.0" minWidth="5.0" prefHeight="200.0" prefWidth="5.0" />
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="btnExtendReturnDate" minWidth="100.0" mnemonicParsing="false" prefWidth="100.0" stylesheets="@styles.css" text="Extend&#10;return date" textFill="WHITE" />
                  <AnchorPane maxHeight="5.0" minHeight="5.0" prefHeight="5.0" />
                  <Button minWidth="100.0" mnemonicParsing="false" stylesheets="@styles.css" text="Extend return&#10;date of all" />
               </children></VBox>
         </children></HBox>
   </children>
</VBox>
